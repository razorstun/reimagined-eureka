{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "plug-n-code"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [
        "../node_modules/ace-builds/src-min/ace.js",
        "../node_modules/ace-builds/src-min/theme-ambiance.js",
        "../node_modules/ace-builds/src-min/theme-gruvbox.js",
        "../node_modules/ace-builds/src-min/theme-solarized_light.js",
        "../node_modules/ace-builds/src-min/theme-chaos.js",
        "../node_modules/ace-builds/src-min/theme-idle_fingers.js",
        "../node_modules/ace-builds/src-min/theme-sqlserver.js",
        "../node_modules/ace-builds/src-min/theme-chrome.js",
        "../node_modules/ace-builds/src-min/theme-iplastic.js",
        "../node_modules/ace-builds/src-min/theme-terminal.js",
        "../node_modules/ace-builds/src-min/theme-clouds.js",
        "../node_modules/ace-builds/src-min/theme-katzenmilch.js",
        "../node_modules/ace-builds/src-min/theme-textmate.js",
        "../node_modules/ace-builds/src-min/theme-clouds_midnight.js",
        "../node_modules/ace-builds/src-min/theme-kr_theme.js",
        "../node_modules/ace-builds/src-min/theme-tomorrow.js",
        "../node_modules/ace-builds/src-min/theme-cobalt.js",
        "../node_modules/ace-builds/src-min/theme-kuroir.js",
        "../node_modules/ace-builds/src-min/theme-tomorrow_night.js",
        "../node_modules/ace-builds/src-min/theme-crimson_editor.js",
        "../node_modules/ace-builds/src-min/theme-merbivore.js",
        "../node_modules/ace-builds/src-min/theme-tomorrow_night_blue.js",
        "../node_modules/ace-builds/src-min/theme-dawn.js",
        "../node_modules/ace-builds/src-min/theme-merbivore_soft.js",
        "../node_modules/ace-builds/src-min/theme-tomorrow_night_bright.js",
        "../node_modules/ace-builds/src-min/theme-dreamweaver.js",
        "../node_modules/ace-builds/src-min/theme-mono_industrial.js",
        "../node_modules/ace-builds/src-min/theme-tomorrow_night_eighties.js",
        "../node_modules/ace-builds/src-min/theme-eclipse.js",
        "../node_modules/ace-builds/src-min/theme-monokai.js",
        "../node_modules/ace-builds/src-min/theme-twilight.js",
        "../node_modules/ace-builds/src-min/theme-github.js",
        "../node_modules/ace-builds/src-min/theme-pastel_on_dark.js",
        "../node_modules/ace-builds/src-min/theme-vibrant_ink.js",
        "../node_modules/ace-builds/src-min/theme-gob.js",
        "../node_modules/ace-builds/src-min/theme-solarized_dark.js",
        "../node_modules/ace-builds/src-min/theme-xcode.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "sass",
    "component": {
    }
  }
}
